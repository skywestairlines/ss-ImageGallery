{
	"name": "skywest/ss-imagegallery",
	"description": "Simple image gallery module for Silverstripe",
	"type": "silverstripe-module",
	"keywords": ["silverstripe", "image", "gallery"],
	"authors": [
		{
			"name": "Aaron Carlino",
			"homepage": "http://leftandmain.com",
			"email": "unclecheese@leftandmain.com"
		},
		{
			"name": "Damian Mooyman",
			"email": "damian.mooyman@gmail.com"
		}
	],
	"require": {
		"silverstripe/framework": "^4.0"
	},
	"extra": {
		"installer-name": "image_gallery",
        "expose": [
            "css",
            "images",
            "javascript",
            "gallery-ui/fancybox/css",
            "gallery-ui/fancybox/javascript",
            "gallery-ui/fancybox/images",
            "gallery-ui/lightbox/css",
            "gallery-ui/lightbox/javascript",
            "gallery-ui/lightbox/images",
            "gallery-ui/nyro_modal/css",
            "gallery-ui/nyro_modal/javascript",
            "gallery-ui/nyro_modal/images",            
            "gallery-ui/shadowbox/css",
            "gallery-ui/shadowbox/javascript",
            "gallery-ui/shadowbox/images",
            "gallery-ui/prettyphoto/css",
            "gallery-ui/prettyphoto/javascript",
            "gallery-ui/prettyphoto/images"
        ]
	}
}
